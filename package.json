{
  "name": "book-clip-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "pretest": "cp .gitignore .cursorignore && tree -a -I 'node_modules|.expo|.git|.DS_Store' > docs/DIRECTORY.md",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "lint": "npm run lint:fix; npm run ts-prune-check; npm run depcheck-check",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "ts-prune": "ts-prune | grep -v '\\+not-found\\|_layout\\|default'",
    "ts-prune-check": "OUTPUT=$(npm run ts-prune -s); if [ -n \"$OUTPUT\" ]; then echo \"以下の未使用エクスポートが見つかりました:\"; echo \"$OUTPUT\"; exit 1; fi",
    "depcheck": "depcheck --ignores=\"jest-expo,@babel/core,@types/jest,@types/react,@types/react-test-renderer,@testing-library/*,jest,react-test-renderer\" --ignore-dirs=\"node_modules,.expo,assets\" --json",
    "depcheck-check": "OUTPUT=$(npm run depcheck -s); if [ \"$(echo $OUTPUT | jq -r '.dependencies | length')\" -gt 0 ] || [ \"$(echo $OUTPUT | jq -r '.devDependencies | length')\" -gt 0 ]; then echo \"以下の未使用パッケージが見つかりました:\"; echo $OUTPUT | jq '.dependencies, .devDependencies'; exit 1; fi"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "expo": "^52.0.37",
    "expo-blur": "~14.0.3",
    "expo-camera": "^16.0.18",
    "expo-constants": "~17.0.7",
    "expo-file-system": "^18.0.11",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image-manipulator": "^13.0.6",
    "expo-linking": "~7.0.5",
    "expo-media-library": "^17.0.6",
    "expo-router": "^4.0.17",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/parser": "^8.26.1",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.1",
    "eslint-config-expo": "~8.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.4",
    "react-test-renderer": "^18.3.1",
    "ts-prune": "^0.10.3",
    "typescript": "^5.3.3"
  },
  "private": true
}
